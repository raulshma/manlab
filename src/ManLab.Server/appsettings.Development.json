{
  "Database": {
    "EnableReadReplicaRouting": true,
    "ReplicaReadProbability": 0.8,
    "ConnectionResiliency": {
      "MaxRetryCount": 3,
      "MaxRetryDelaySeconds": 30,
      "EnableDetailedErrors": true
    }
  },
  "SignalR": {
    "EnableRedisBackplane": false,
    "RedisChannelPrefix": "ManLab"
  },
  "Performance": {
    "EnableGcLatencyMode": "LowLatency",
    "ArrayPoolRentThreshold": 1024
  },
  "BinaryDistribution": {
    "RootPath": "",
    "DefaultChannel": "stable",
    "EnableLegacyFallback": true
  },
  "Discord": {
    "WebhookUrl": ""
  },
  "Retention": {
    "TelemetrySnapshotDays": 30,
    "ServiceStatusSnapshotDays": 30,
    "SmartDriveSnapshotDays": 30,
    "GpuSnapshotDays": 30,
    "UpsSnapshotDays": 30,
    "CleanupIntervalMinutes": 60
  },
  "SpeedTest": {
    "LocateBaseUrl": "https://locate.measurementlab.net",
    "ServiceName": "ndt",
    "ServiceType": "ndt7",
    "ClientName": "manlab",
    "ClientVersion": "",
    "ClientLibraryName": "manlab-server",
    "ClientLibraryVersion": "",
    "DownloadSizeBytes": 10000000,
    "UploadSizeBytes": 5000000,
    "LatencySamples": 3,
    "MaxSizeBytes": 50000000,
    "MaxTestSeconds": 13,
    "LocateTimeoutSeconds": 15
  },
  "PublicIp": {
    "TimeoutSeconds": 5
  },
  "NetworkRateLimit": {
    "MaxConcurrentScans": 1,
    "PingRequestsPerMinute": 60,
    "TracerouteRequestsPerMinute": 20,
    "PortScanRequestsPerMinute": 10,
    "SubnetScanRequestsPerMinute": 5,
    "DiscoveryRequestsPerMinute": 10,
    "SpeedTestRequestsPerMinute": 5
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=manlab;Username=manlab;Password=manlab_secret"
  }
}
